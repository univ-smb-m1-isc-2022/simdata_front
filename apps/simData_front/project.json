{
  "name": "simData_front",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "generators": {
    "@schematics/angular:component": {
      "style": "scss"
    }
  },
  "sourceRoot": "apps/simData_front/src",
  "prefix": "app",
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "options": {
        "outputPath": "dist/apps/simData_front",
        "index": "apps/simData_front/src/index.html",
        "main": "apps/simData_front/src/main.ts",
        "polyfills": "apps/simData_front/src/polyfills.ts",
        "tsConfig": "apps/simData_front/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          "apps/simData_front/src/favicon.ico",
          "apps/simData_front/src/assets"
        ],
        "styles": [
          "apps/simData_front/src/style/main.scss"
        ],
        "stylePreprocessorOptions": {
          "includePaths": ["apps/simData_front/src/style"]
        },
        "scripts": []
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500mb",
              "maximumError": "1gb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2mb",
              "maximumError": "4mb"
            }
          ],
          "fileReplacements": [
            {
              "replace": "apps/simData_front/src/environments/environment.ts",
              "with": "apps/simData_front/src/environments/environment.prod.ts"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "browserTarget": "simData_front:build:production"
        },
        "development": {
          "browserTarget": "simData_front:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "simData_front:build"
      }
    },
    "test": {
      "executor": "@angular-devkit/build-angular:karma",
      "options": {
        "main": "apps/simData_front/src/test.ts",
        "polyfills": "apps/simData_front/src/polyfills.ts",
        "tsConfig": "apps/simData_front/tsconfig.spec.json",
        "karmaConfig": "apps/simData_front/karma.conf.js",
        "inlineStyleLanguage": "scss",
        "assets": [
          "apps/simData_front/src/favicon.ico",
          "apps/simData_front/src/assets"
        ],
        "styles": [
          "apps/simData_front/src/style/main.scss"
        ],
        "stylePreprocessorOptions": {
          "includePaths": ["apps/simData_front/src/style"]
        },
        "scripts": []
      }
    }
  }
}
